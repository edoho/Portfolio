 import java.util.Scanner;
 
 public class For_Loop {
	 public static void main(String[] args) {
		 Scanner Input = new Scanner(System.in);
		
		//Set up variable; While Loop; Increment or Decrement
		//can use ++, +=, or -=
		for (int a = 0; a < 100; a++) {
			System.out.println(a);
		}
		
	}	 
}	
