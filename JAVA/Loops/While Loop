 import java.util.Scanner;
 
 public class While_Loop {
	 public static void main(String[] args) {
		 Scanner Input = new Scanner(System.in);
		 
		 int a = 0;
		 //basically repeating IF statement
		while (a < 100) {
			System.out.prinln(a);
			//a = a + 1;
			a++;
		}
		
		System.out.println("Loop finished.");
	}	
}
